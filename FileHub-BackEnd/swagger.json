{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "File Hub API"
  },
  "host": "localhost:3000",
  "schemes": [
    "http"
  ],
  "paths": {
    "/file": {
      "post": {
        "tags": [
          "File"
        ],
        "summary": "Upload a file",
        "description": "uploads a file",
        "operationId": "uploadFile",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "in": "formData",
            "name": "fileToUpload",
            "description": "File to upload",
            "required": true,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          },
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Health check",
        "description": "Verifies if service is healthy. Returns 200 OK if service is available.",
        "produces": [
          "application/xml",
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Indicates service is available"
          }
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}